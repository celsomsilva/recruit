    "chalk": "5.4.1",
    "css-loader": "7.1.2",
    "dotenv": "16.5.0",
    "eslint": "^9.28.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "6.2.0",
    "firebase": "^11.9.0",
    "fs-extra": "11.3.0",
    "html-webpack-plugin": "5.6.3",
    "jest": "29.7.0",
    "mobx": "^6.13.7",
    "mobx-react": "^9.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "5.0.2",
